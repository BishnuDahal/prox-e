{
  "name": "prox-e",
  "version": "0.0.1",
  "main": "index.js",
  "description": "Simple REST APIs to extend CST's core functionalities",
  "author": "Risav Karna <risavkarna@gmail.com>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "github.com/covidsimteam/prox-e.git"
  },
  "keywords": [
    "proxy",
    "cache",
    "middleware",
    "bridge",
    "gateway"
  ],
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc",
    "start": "node build/src/server.js",
    "start-hoodie": "hoodie",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@cloudant/couchbackup": "^2.5.2",
    "@hoodie/client": "^10.2.0",
    "@liftr/tscov": "^1.4.7",
    "@octokit/rest": "^18.0.3",
    "@types/echarts": "^4.6.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "bunyan-cloudant": "^1.0.2",
    "cors": "^2.8.5",
    "couchexport": "0.0.2",
    "couchimport": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "hapi": "^18.1.0",
    "hoodie": "^28.2.10",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mem": "^6.1.0",
    "minimist": "^1.2.5",
    "pouchdb": "^7.2.2",
    "pouchdb-browser": "^7.2.2",
    "request": "^2.88.2",
    "rxjs": "^6.6.2",
    "rxjs-compat": "^6.6.2",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/bunyan": "^1.8.6",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.27",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "hoodie": {
    "address": "127.0.0.1",
    "port": 8080,
    "data": ".hoodie",
    "public": "public",
    "dbAdapter": "pouchdb-adapter-memory",
    "inMemory": true,
    "loglevel": "warn",
    "url": "http://127.0.0.1:8080/hood",
    "adminPassword": "public"
  }
}
